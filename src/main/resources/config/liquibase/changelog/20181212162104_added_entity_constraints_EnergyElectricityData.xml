<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity EnergyElectricityData.
    -->
    <changeSet id="20181212162104-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="voltage_id"
                                 baseTableName="energy_electricity_data"
                                 constraintName="fk_energy_electricity_data_voltage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="energy_cycle_data"/>

        <addForeignKeyConstraint baseColumnNames="tariff_id"
                                 baseTableName="energy_electricity_data"
                                 constraintName="fk_energy_electricity_data_tariff_id"
                                 referencedColumnNames="id"
                                 referencedTableName="energy_tariff_data"/>

        <addForeignKeyConstraint baseColumnNames="cycle_id"
                                 baseTableName="energy_electricity_data"
                                 constraintName="fk_energy_electricity_data_cycle_id"
                                 referencedColumnNames="id"
                                 referencedTableName="energy_cycle_data"/>

        <addForeignKeyConstraint baseColumnNames="time_period_id"
                                 baseTableName="energy_electricity_data"
                                 constraintName="fk_energy_electricity_data_time_period_id"
                                 referencedColumnNames="id"
                                 referencedTableName="energy_time_period_data"/>

    </changeSet>
</databaseChangeLog>
